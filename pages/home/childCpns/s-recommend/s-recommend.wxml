<!--pages/home/childCpns/s-recommend/s-recommend.wxml-->
<view class="recommend">
  <block wx:for="{{recommends}}" wx:key='image'>
    <view class="recommend-item">
      <image class='image' src="{{item.image}}" mode="widthFix" bind:load='handleImageLoad'></image>
      <view class="title">{{item.title}}</view>
    </view>
  </block>
</view>
