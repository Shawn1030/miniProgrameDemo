<!--component/tab-bar/tab-bar.wxml-->

<view class="tab-control">
  <block >
    <view class="tab-item {{currentIndex===index?'active':''}}" wx:for='{{titles}}' wx:key='*this'
    bind:tap='itemClick' 
    data-index="{{index}}">
    <text class="text">{{item}}</text>
    </view>
  </block>
</view>
